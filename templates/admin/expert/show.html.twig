{% extends 'admin/layout.html.twig' %}

{% block title %}Expert
{% endblock %}

{% block body %}
	<h1>Expert</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ expert.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ expert.name }}</td>
			</tr>
			<tr>
				<th>LastName</th>
				<td>{{ expert.lastName }}</td>
			</tr>
			<tr>
				<th>Profession</th>
				<td>{{ expert.profession }}</td>
			</tr>
			<tr>
				<th>NumeroSIRET</th>
				<td>{{ expert.numeroSIRET }}</td>
			</tr>
			<tr>
				<th>Address</th>
				<td>{{ expert.address }}</td>
			</tr>
			<tr>
				<th>Postal_code</th>
				<td>{{ expert.postalCode }}</td>
			</tr>
			<tr>
				<th>City</th>
				<td>{{ expert.city }}</td>
			</tr>
			<tr>
				<th>Email</th>
				<td>{{ expert.email }}</td>
			</tr>
			<tr>
				<th>Password</th>
				<td>{{ expert.password }}</td>
