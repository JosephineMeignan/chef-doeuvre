{{ form_start(form) }}
    {# {{ form_widget(form) }} #}
    <div class="my-custom-class-for-errors">
        {{ form_errors(form) }}
    </div>

    <div class="row">
        <div class="col">
            {{ form_row(form.name) }}
        </div>
        <div class="col" id="some-custom-id">
            {{ form_row(form.lastName) }}
        </div>
                <div class="col">
            {{ form_row(form.profession) }}
        </div>
        <div class="col" id="some-custom-id">
            {{ form_row(form.themeId) }}
        </div>
                <div class="col">
            {{ form_row(form.numeroSIRET) }}
        </div>
        <div class="col" id="some-custom-id">
            {{ form_row(form.address) }}
        </div>
                <div class="col">
            {{ form_row(form.postal_code) }}
        </div>
        <div class="col" id="some-custom-id">
            {{ form_row(form.city) }}
        </div>
                <div class="col" id="some-custom-id">
            {{ form_row(form.email) }}
        </div>
                <div class="col">
            {{ form_row(form.password) }}
        </div>
    </div>
    <button class="btn">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
